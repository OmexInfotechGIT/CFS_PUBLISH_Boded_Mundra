@{
    CFS.Models.ReportsModal ReportsModal = new CFS.Models.ReportsModal();

    ViewBag.Title = "Custom Report";
}
@using (Html.BeginForm("CustomReport", "Reports", FormMethod.Post))
{
    <div class="">
        <div class="box">
            <div class="box-body">
                <input type="hidden" id="msg" name="msg" value="@ViewBag.Msg">
                @*@Html.Partial("_ReportsModalDate")*@
                @*@Html.Partial("_ReportsModalSearchClearButton")*@
                <div class="col-md-4">
                    <div class="form-group" style="padding-top: 24px;">
                        <input type="hidden" id="msg" name="msg" value="@ViewBag.Msg">                        
                        <button style="margin-right:10px;" type="submit" id="IsGenerate" onclick="return Validate();" name="IsGenerate" value="Y" class="btn btn-primary">Generate Reports</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<script src="@CFS.Com.ClsCommon.GetCacheVersion("FormJs/ReportsModal.js")"></script>

