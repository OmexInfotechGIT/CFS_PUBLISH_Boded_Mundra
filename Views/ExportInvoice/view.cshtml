                                            <tr class="text-right">
                                                <td><b> Net Amount :</b></td>
                                                <td id="FinalNetAmount">@(Math.Round(Convert.ToDecimal(TotalAmount)).ToString("0.00")) </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
