                                                StrAutocomplete.Append(@"  Autocompletebox('WHLocation" + Q + @"', 'WHLocationID" + Q + @"', 'trnTruckDestuffing', 'GetLocation');
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           Autocompletebox('PackingType" + Q + @"', 'PackingTypeID" + Q + @"', 'trnTruckDestuffing', 'GetPackingType');
