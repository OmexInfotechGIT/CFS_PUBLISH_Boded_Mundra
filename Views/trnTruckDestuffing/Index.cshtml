@model CFS.Models.trnTruckDestuffing
@using System.Globalization;
@{
    ViewBag.Title = "Truck Destuffing List";
    ViewBag.gblBlnView = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.View);
    ViewBag.gblBlnEdit = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Edit);
    ViewBag.gblBlnAdd = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Add);
    ViewBag.gblBlnDelete = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Delete);
    if (!ViewBag.gblBlnView)
    {
        Response.Redirect(CFS.Com.ClsCommon.GetRootPath() + "Error/unauthorize/");
    }
}
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header" id="divSearch">
                    <div class="pull-right">
                        <div style="display: inline-flex">
                            <div class="input-group input-group-sm">
                                <input id="txtSearch" Name="txtSearch" type="text" class="form-control" value="@Model.pagger.Search">
                                <span class="input-group-btn">
                                    <button type="button" onclick="SearchData()" class="btn btn-primary btn-flat" id="btnSearch">Go!</button>
                                </span>
                            </div>
                            <div>&nbsp;</div>
                            <div>
                                @if (ViewBag.gblBlnAdd)
                                {
                                    <a id="btnAdd" href="@Url.Action("Action", "trnTruckDestuffing", new { doaction = "add" } )" class="btn btn-primary btn-sm" style="color: #fff" data-toggle="tooltip" title="Add New Truck Destuffing"><i class="fa fa-plus"></i></a>
                                }
                            </div>
                        </div>
                    </div>
                    <br />
                </div>
                <div class="box-body">
                    <div style="overflow-x:scroll">
                        <table id="example2" style="white-space:nowrap" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.trnTruckDestuffingNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.trntruckdestuffingno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.trntruckdestuffingno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> Truck Destuffing No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.TruckNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.truckno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.truckno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> Truck No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.trnDocumentNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.trndocumentno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.trndocumentno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> NOC No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.BOENo", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.boeno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.boeno" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> In-BOE No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.BeginDestuffDate", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.begindestuffdate" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.begindestuffdate" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> Destuff Begin Date</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "trnTruckDestuffing", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "A.EndDestuffDate", SortOrder = (Model.pagger.SortColumn.ToLower() == "a.enddestuffdate" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "a.enddestuffdate" && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i> Destuff End Date</a>
                                    </th>
                                    <th class="freezeLastColumn">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < Model.MyDatatable.Rows.Count; i++)
                                {
                                    <tr>
                                        <td>@(Model.MyDatatable.Rows[i]["trnTruckDestuffingPrefix"].ToString() + Model.MyDatatable.Rows[i]["trnTruckDestuffingNo"].ToString())</td>
                                        <td>@Convert.ToString(Model.MyDatatable.Rows[i]["TruckNo"])</td>
                                        <td>@Convert.ToString(Model.MyDatatable.Rows[i]["trnDocumentNo"])</td>
                                        <td>@Convert.ToString(Model.MyDatatable.Rows[i]["BOENo"])</td>
                                        <td>@Convert.ToDateTime(Model.MyDatatable.Rows[i]["BeginDestuffDate"]).ToString(CFS.Com.ClsCommon.CSProjectDateTimeFormat, CultureInfo.InvariantCulture)</td>
                                        <td>@Convert.ToDateTime(Model.MyDatatable.Rows[i]["EndDestuffDate"]).ToString(CFS.Com.ClsCommon.CSProjectDateTimeFormat, CultureInfo.InvariantCulture)</td>
                                        <td class="freezeLastColumnwithWhite">
                                            @if (ViewBag.gblBlnEdit && (Convert.ToBoolean(Model.MyDatatable.Rows[i]["IsExbondDone"])))
                                            {
                                                <a href="@Url.Action("Action", "trnTruckDestuffing", new { id = Model.MyDatatable.Rows[i]["trnTruckDestuffingID"], pg = Model.pagger.CurrentPage, search = Model.pagger.Search, doaction = "edit", @IsSearch = true,@WorkOrdersearchID = Convert.ToString(Model.MyDatatable.Rows[i]["WorkOrderID"]) + "_0_" + Convert.ToString(Model.MyDatatable.Rows[i]["trnCargoGateInID"]) ,@WorkOrderNo = Convert.ToString(Model.MyDatatable.Rows[i]["WorkOrderNo"]) + " - " + Convert.ToString(Model.MyDatatable.Rows[i]["TruckNo"]) + " - " + Convert.ToString(Model.MyDatatable.Rows[i]["CargoGateInNo"]), @IsWorkOrderOrGateIn = false })"><i title="Click to view" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-edit"></i></a>
                                            }
                                            @if (ViewBag.gblBlnView)
                                            {
                                                <a href="@Url.Action("view", "trnTruckDestuffing", new { id = Model.MyDatatable.Rows[i]["trnTruckDestuffingID"] , pg = Model.pagger.CurrentPage, search = Model.pagger.Search, doaction = "edit", @IsSearch = true,@WorkOrdersearchID = Convert.ToString(Model.MyDatatable.Rows[i]["WorkOrderID"]) + "_0_" + Convert.ToString(Model.MyDatatable.Rows[i]["trnCargoGateInID"]) ,@WorkOrderNo = Convert.ToString(Model.MyDatatable.Rows[i]["WorkOrderNo"]) + " - " + Convert.ToString(Model.MyDatatable.Rows[i]["TruckNo"]) + " - " + Convert.ToString(Model.MyDatatable.Rows[i]["CargoGateInNo"]), @IsWorkOrderOrGateIn = false })"><i title="Click to view" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-eye"></i></a>
                                            }
                                            <a><i data-toggle="modal" data-target="#myModal" onclick="ShowHistory('trnTruckDestuffing','@Model.MyDatatable.Rows[i]["trnTruckDestuffingID"]')" title="History" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-info"></i></a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                    @Html.Partial("_pagination")
                </div>
            </div>
        </div>
    </div>
</section>
<script>
    var doaction = '@Model.doaction';
</script>
<script src="@CFS.Com.ClsCommon.GetCacheVersion("FormJs/trnTruckDestuffing.js")"></script>

