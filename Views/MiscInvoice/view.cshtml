                        @Html.LabelFor(Model => Model.ConsolerName, new { @for = "ConsolerName" }) : <span>@Model.ConsolerName</span>
                        @Html.HiddenFor(Model => Model.ConsolerID, new { @id = "ConsolerID", @name = "ConsolerID" })

