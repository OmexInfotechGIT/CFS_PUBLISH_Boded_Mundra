@model CFS.Models.ExcessCargoRemoval
@{
    ViewBag.Title = "Excess Cargo Removal";
    ViewBag.gblBlnView = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.View);
    ViewBag.gblBlnEdit = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Edit);
    ViewBag.gblBlnAdd = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Add);
    ViewBag.gblBlnDelete = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Delete);
    ViewBag.gblBlnApprove = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Approve);
    ViewBag.gblBlnUnApprove = CFS.Com.ClsCommon.getaccesspage(CFS.Com.ClsCommon.Controller(), CFS.Com.ClsCommon.AccessType.Unapprove);
    if (!ViewBag.gblBlnView)
    {
        Response.Redirect(CFS.Com.ClsCommon.GetRootPath() + "Error/unauthorize/");
    }
}
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <div class="pull-right">
                        <div style="display: inline-flex">
                            <div class="input-group input-group-sm">
                                <input id="txtSearch" Name="txtSearch" type="text" class="form-control" value="@Model.pagger.Search">
                                <span class="input-group-btn">
                                    <button type="button" onclick="SearchData()" class="btn btn-primary btn-flat">Go!</button>
                                </span>
                            </div>
                            <div>&nbsp;</div>
                            <div>
                                @if (ViewBag.gblBlnAdd)
                                {
                                    <a href="@Url.Action("Action", "ExcessCargoRemoval", new { doaction = "add" } )" class="btn btn-primary btn-sm" style="color: #fff" data-toggle="tooltip" title="Add New Cargo Repacking"><i class="fa fa-plus"></i></a>
                                }
                            </div>
                        </div>
                    </div>
                    <br />
                </div>
                <div class="box-body">
                    <div style="overflow-x:scroll">
                        <table id="example2" style="white-space:nowrap" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "ExcessCargoRemovalNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "ExcessCargoRemovalNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "ExcessCargoRemovalNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>ExcessCargoRemoval No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "CreatedDate", SortOrder = (Model.pagger.SortColumn.ToLower() == "CreatedDate".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "CreatedDate".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>ExcessCargoRemoval Date</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "SpaceCertificateNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "SpaceCertificateNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "SpaceCertificateNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>NOC No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "BatchNo", SortOrder = (Model.pagger.SortColumn.ToLower() == "BatchNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "BatchNo".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>Batch No</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "CHAName", SortOrder = (Model.pagger.SortColumn.ToLower() == "CHAName".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "CHAName".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>CHAName</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "Forwarder", SortOrder = (Model.pagger.SortColumn.ToLower() == "Forwarder".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "Forwarder".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>Forwarder</a>
                                    </th>
                                    <th>
                                        <a class="text-black" href="@Url.Action("index", "ExcessCargoRemoval", new {  pg = Model.pagger.CurrentPage, search = Model.pagger.Search,SortColumn = "ImporterName", SortOrder = (Model.pagger.SortColumn.ToLower() == "ImporterName".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc")} )"><i title="Click to sorting" class="fa fa-sort-amount-@((Model.pagger.SortColumn.ToLower() == "ImporterName".ToLower() && Model.pagger.SortOrder.ToLower() =="asc" ? "desc" : "asc").ToString().ToLower())"></i>ImporterName</a>
                                    </th>
                                    <th class="freezeLastColumn">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < Model.MyDatatable.Rows.Count; i++)
                                {
                                    <tr>
                                        <td>@Model.MyDatatable.Rows[i]["ExcessCargoRemovalPrefix"]@Model.MyDatatable.Rows[i]["ExcessCargoRemovalNo"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["CreatedDate"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["SpaceCertificateNo"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["BatchNo"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["CHAName"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["Forwarder"]</td>
                                        <td>@Model.MyDatatable.Rows[i]["ImporterName"]</td>
                                        <td class="freezeLastColumnwithWhite">

                                            <a href="@Url.Action("Action", "ExcessCargoRemoval", new { id = Model.MyDatatable.Rows[i]["ExcessCargoRemovalID"], pg = Model.pagger.CurrentPage, search = Model.pagger.Search, doaction = "edit" })"><i title="Click to Edit" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-edit"></i></a>
                                            <a href="@Url.Action("view", "ExcessCargoRemoval", new { id = Model.MyDatatable.Rows[i]["ExcessCargoRemovalID"] , pg = Model.pagger.CurrentPage, search = Model.pagger.Search, doaction = "view", @IsSearch = true })"><i title="Click to view" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-eye"></i></a>
                                            <a><i data-toggle="modal" data-target="#myModal" onclick="ShowHistory('ExcessCargoRemoval','@Model.MyDatatable.Rows[i]["ExcessCargoRemovalID"]')" title="History" style=" font-size: x-large; cursor: pointer; vertical-align: middle;" class="fa fa-info"></i></a>
                                            <a href="@Url.Action("delete", "ExcessCargoRemoval", new { id = Model.MyDatatable.Rows[i]["ExcessCargoRemovalID"], pg = Model.pagger.CurrentPage, search = Model.pagger.Search })" onclick="return confirm('Are you sure want to delete this record?');"><i title="Click to Delete" style="font-size: x-large; cursor: pointer; vertical-align: middle; color:red" class="fa fa-trash-o"></i></a>
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                    @Html.Partial("_pagination")
                </div>
            </div>
        </div>
    </div>
</section>
<script>
    //var IsSearch = '@*@Model.IsSearch.ToString().ToLower()*@';
    var doaction = '@Model.doaction'; var IsView = "False";</script>
<script src="@CFS.Com.ClsCommon.GetCacheVersion("FormJs/ExcessCargoRemoval.js")"></script>
